function poundsKilo(pound:number) :number{
    let kilo: number = 0.453592;
    return pound*kilo;
}
console.log(poundsKilo(15));

function inchesCentimeter(inches:number): number{
    let centimeter: number=2.54;
    return inches*centimeter;
}
console.log(inchesCentimeter(5));

function fahrenheitCelsius(fahrenheit:number): number{
    let fc:number =32;
    let celsius= (fc-32)*5/9
    return fc*celsius;
}
console.log(fahrenheitCelsius(15));

function feetMeter(feet): number{
    let meter:number =0.3048;
    return meter/feet;
}
console.log(feetMeter(2));

function oddEven(num1:number): string{
    if (num1%2==0){
    return "even";
    }
    else {
    return "odd";}
}
console.log(oddEven(7));

  function fizzBuzz(num:number ): string{
    if (num%3 && num%5){
        return "Fizz-Buzz";
    }
    
    if (num%5==0){
        return "Buzz";
    }
    if (num%3==0){
        return "Fizz";
    }
    else {
        return "its just a number";
   
    }
}
    console.log(fizzBuzz(6));


  